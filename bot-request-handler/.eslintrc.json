{
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": [
      "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint"
    ],
    "rules": {
      "import/no-unresolved": 0,
      "import/extensions": 0,
      "no-shadow": 0,
      "@typescript-eslint/no-shadow": "error",
      "no-unused-vars": 0,
      "@typescript-eslint/no-unused-vars": "error"
    }
}